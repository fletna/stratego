module ex-2-free-var
  signature
    sorts Exp
    
    constructors
      Plus : Exp * Exp -> Exp
    
  
  rules
           create-plus : (i,j) -> Plus(<cast-value(strategoGradual--is-a-Exp|"Exp")> i, <cast-value(strategoGradual--is-a-Exp|"Exp")> j)
           
  rules
           cast-value(s | typ) = s <+ ?a; report-failure( |<concat-strings> ["Cast to ",typ," failed"], a)
           
           strategoGradual--is-a-Exp =
              ?Plus(_, _)
           
           

module Stratego-Sugar-Syntax

language
                  
  line comment  : "//"
  block comment : "/*" * "*/"
  fences        : [ ]
                  ( )
                  { }
                  {| |}
                  |[ ]|
                  $[ ]
                  ${ }
                  $< >
                  $( )
  
  indent after:
    then
    in
    else
    ":"
    "="
    <
    >
    rules
    strategies
    overlays
    constructors
    imports
